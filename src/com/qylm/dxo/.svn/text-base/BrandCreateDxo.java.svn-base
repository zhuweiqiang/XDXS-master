package com.qylm.dxo;

import com.qylm.dto.baseSet.BrandCreateDto;
import com.qylm.entity.Brand;

public final class BrandCreateDxo {

	public static void dtoToEntity(BrandCreateDto dto, Brand entity) {
		entity.setName(dto.getName());
		entity.setRemark(dto.getRemark());
		entity.setCreater(dto.getCreater());
		entity.setBelongingUser(dto.getBelongingUser());
	}

	public static void entityToDto(Brand entity, BrandCreateDto dto) {
		dto.setName(entity.getName());
		dto.setRemark(entity.getRemark());
		dto.setCreater(entity.getCreater());
		dto.setBelongingUser(entity.getBelongingUser());
	}

}
