package com.qylm.dxo;

import com.qylm.dto.baseSet.BrandViewDto;
import com.qylm.entity.Brand;

public final class BrandViewDxo {

	public static void dtoToEntity(BrandViewDto dto, Brand entity) {
		entity.setName(dto.getName());
		entity.setRemark(dto.getRemark());
		entity.setCreater(dto.getCreater());
	}

	public static void entityToDto(Brand entity, BrandViewDto dto) {
		dto.setName(entity.getName());
		dto.setRemark(entity.getRemark());
		dto.setCreater(entity.getCreater());
	}

}
