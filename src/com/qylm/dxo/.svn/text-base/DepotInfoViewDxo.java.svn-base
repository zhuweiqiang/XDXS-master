package com.qylm.dxo;

import com.qylm.dto.depot.DepotInfoViewDto;
import com.qylm.entity.DepotInfo;

public final class DepotInfoViewDxo {

	public static void dtoToEntity(DepotInfoViewDto dto, DepotInfo entity) {
		entity.setName(dto.getName());
		entity.setRemark(dto.getRemark());
		entity.setCreater(dto.getCreater());
	}

	public static void entityToDto(DepotInfo entity, DepotInfoViewDto dto) {
		dto.setName(entity.getName());
		dto.setRemark(entity.getRemark());
		dto.setCreater(entity.getCreater());
	}

}
