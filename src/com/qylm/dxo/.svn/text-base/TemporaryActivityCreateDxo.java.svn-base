package com.qylm.dxo;

import com.qylm.dto.baseSet.TemporaryActivityCreateDto;
import com.qylm.entity.TemporaryActivity;

public final class TemporaryActivityCreateDxo {

	public static void dtoToEntity(TemporaryActivityCreateDto dto, TemporaryActivity entity) {
		entity.setName(dto.getName());
		entity.setRemark(dto.getRemark());
		entity.setMoney(dto.getMoney());
		entity.setState(dto.isState());
		entity.setTemporaryActivityDetailList(dto.getTemporaryActivityDetailList());
		entity.setCreater(dto.getCreater());
		entity.setBelongingUser(dto.getBelongingUser());
	}

	public static void entityToDto(TemporaryActivity entity, TemporaryActivityCreateDto dto) {
		dto.setName(entity.getName());
		dto.setRemark(entity.getRemark());
		dto.setMoney(entity.getMoney());
		dto.setState(entity.isState());
		dto.setTemporaryActivityDetailList(entity.getTemporaryActivityDetailList());
		dto.setCreater(entity.getCreater());
		dto.setBelongingUser(entity.getBelongingUser());
	}

}
