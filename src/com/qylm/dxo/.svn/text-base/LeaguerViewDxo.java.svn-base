package com.qylm.dxo;

import com.qylm.dto.baseSet.LeaguerViewDto;
import com.qylm.entity.Leaguer;

public final class LeaguerViewDxo {

	public static void dtoToEntity(LeaguerViewDto dto, Leaguer entity) {
		entity.setLevel(dto.getLevel());
		entity.setRebate(dto.getRebate());
		entity.setCreater(dto.getCreater());
		entity.setBelongingUser(dto.getBelongingUser());
	}

	public static void entityToDto(Leaguer entity, LeaguerViewDto dto) {
		dto.setLevel(entity.getLevel());
		dto.setRebate(entity.getRebate());
		dto.setCreater(entity.getCreater());
		dto.setBelongingUser(entity.getBelongingUser());
	}

}
