package com.qylm.common.address;

import java.io.Serializable;

import com.qylm.common.SelectItemCreator;

/**
 * 全国所有的省
 * @author smj
 *
 */
public class Province implements SelectItemCreator, Serializable {

	/**
	 * serialVersionUID
	 */
	private static final long serialVersionUID = 3000365466766221552L;
	
	/**
	 * 省ID
	 */
	private Integer provinceId;
	
	/**
	 * 省名称
	 */
	private String name;
	
	/**
	 * 版本
	 */
	private Integer version;

	/**
	 * get provinceId
	 * @return the provinceId
	 */
	public Integer getProvinceId() {
		return provinceId;
	}

	/**
	 * set provinceId
	 * @param provinceId the provinceId to set
	 */
	public void setProvinceId(Integer provinceId) {
		this.provinceId = provinceId;
	}

	/**
	 * get name
	 * @return the name
	 */
	public String getName() {
		return name;
	}

	/**
	 * set name
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}

	/**
	 * get version
	 * @return the version
	 */
	public Integer getVersion() {
		return version;
	}

	/**
	 * set version
	 * @param version the version to set
	 */
	public void setVersion(Integer version) {
		this.version = version;
	}

	public String getValue() {
		return String.valueOf(this.provinceId);
	}

	public String getLabel() {
		return this.name;
	}

}
