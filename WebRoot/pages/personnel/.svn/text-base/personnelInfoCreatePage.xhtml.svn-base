<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:p="http://primefaces.org/ui"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:pe="http://primefaces.org/ui/extensions"
	xmlns:t="http://myfaces.apache.org/tomahawk"
	xmlns:rw="http://www.qylm.com"
	template="/WEB-INF/facelets/templates/template.xhtml">
	<ui:param name="title" value="人事信息信息登录" />
	<ui:define name="tableCenter" >
		<t:saveState value="#{personnelInfoCreateBean.personnelInfoCreateDto}" />
		<div class="buttonNorth">
			<p:commandButton value="返回" action="#{personnelInfoCreateBean.back}" immediate="true" ajax="false" styleClass="backBtn" />
			<p:commandButton value="添加" action="#{personnelInfoCreateBean.newPersonnelInfo}" immediate="true" update="@form" styleClass="createBtn" 
				rendered="#{userBean.isCheckPermission('personnel_personnelInfo', '添加')}"/>
			<p:commandButton value="保存" action="#{personnelInfoCreateBean.savePersonnelInfo}" ajax="false" styleClass="saveBtn" 
				rendered="#{userBean.isCheckPermission('personnel_personnelInfo', '添加') or userBean.isCheckPermission('personnel_personnelInfo', '修改')}"/>
		</div>
		<div class="tableCenter">
			<p:focus for="workNumber" />
			<p:panelGrid id="manageGrid" styleClass="dataTable"> 
				<p:row>
					<p:column styleClass="column1">
						<p:outputLabel for="workNumber" value="工号" />
					</p:column>
					<p:column styleClass="column2">
						<p:inputText styleClass="inputText" id="workNumber" size="10" maxlength="10"
							value="#{personnelInfoCreateBean.personnelInfoCreateDto.workNumber}" label="工号必须输入" required="true"/>
					</p:column>
					<p:column styleClass="column1">
						<p:outputLabel for="name" value="姓名" />
					</p:column>
					<p:column styleClass="column4">
						<p:inputText styleClass="inputText" id="name" size="20" maxlength="20"
							value="#{personnelInfoCreateBean.personnelInfoCreateDto.name}" label="姓名必须输入" required="true"/>
					</p:column>
				</p:row>
				<p:row>
					<p:column styleClass="column1">
						<p:outputLabel for="identification" value="身份证号" />
					</p:column>
					<p:column styleClass="column2">
						<p:inputText styleClass="inputText" id="identification" size="20" maxlength="18"
							value="#{personnelInfoCreateBean.personnelInfoCreateDto.identification}">
							<f:validator validatorId="idCardValidator" />
						</p:inputText>
					</p:column>
					<p:column styleClass="column1">
						<p:outputLabel for="sex" value="性别" />
					</p:column>
					<p:column styleClass="column4">
						<p:selectOneMenu id="sex" value="#{personnelInfoCreateBean.personnelInfoCreateDto.sex}">
							<f:selectItems value="#{genericCodeBean.c0102}" />
						</p:selectOneMenu>
					</p:column>
				</p:row>
				<p:row>
					<p:column styleClass="column1">
						<p:outputLabel for="nation" value="民族" />
					</p:column>
					<p:column styleClass="column2">
						<p:selectOneMenu id="nation" value="#{personnelInfoCreateBean.personnelInfoCreateDto.nation}">
							<f:selectItems value="#{genericCodeBean.c0101}" />
						</p:selectOneMenu>
					</p:column>
					<p:column styleClass="column1">
						<p:outputLabel for="marriage" value="婚否" />
					</p:column>
					<p:column styleClass="column4">
						<p:selectOneMenu id="marriage" value="#{personnelInfoCreateBean.personnelInfoCreateDto.marriage}">
							<f:selectItems value="#{genericCodeBean.c0104}" />
						</p:selectOneMenu>
					</p:column>
				</p:row>
				<p:row>
					<p:column styleClass="column1">
						<p:outputLabel for="birthDate" value="出身日期" />
					</p:column>
					<p:column styleClass="column2">
						<p:calendar navigator="true" id="birthDate" size="10" maxlength="10"
							locale="tr" value="#{personnelInfoCreateBean.personnelInfoCreateDto.birthDate}"
							pattern="yyyy-MM-dd"/>
					</p:column>
					<p:column styleClass="column1">
						<p:outputLabel for="politicalStatus" value="政治面貌" />
					</p:column>
					<p:column styleClass="column4">
						<p:selectOneMenu id="politicalStatus" value="#{personnelInfoCreateBean.personnelInfoCreateDto.politicalStatus}">
							<f:selectItems value="#{genericCodeBean.c0103}" />
						</p:selectOneMenu>
					</p:column>
				</p:row>
				<p:row>
					<p:column styleClass="column1">
						<p:outputLabel for="formalSchooling" value="学历" />
					</p:column>
					<p:column styleClass="column2">
						<p:selectOneMenu id="formalSchooling" value="#{personnelInfoCreateBean.personnelInfoCreateDto.formalSchooling}">
							<f:selectItems value="#{genericCodeBean.c0105}" />
						</p:selectOneMenu>
					</p:column>
					<p:column styleClass="column1">
						<p:outputLabel for="mobile" value="联系电话" />
					</p:column>
					<p:column styleClass="column4">
						<p:inputText styleClass="inputText" id="mobile" size="15" maxlength="11"
							value="#{personnelInfoCreateBean.personnelInfoCreateDto.mobile}">
							<f:validator validatorId="numberValidator" />
						</p:inputText>
					</p:column>
				</p:row>
				<p:row>
					<p:column styleClass="column1">
						<p:outputLabel for="type" value="人员类型" />
					</p:column>
					<p:column styleClass="column4" colspan="3">
						<p:selectOneMenu id="type" value="#{personnelInfoCreateBean.personnelInfoCreateDto.type}">
							<f:selectItems value="#{genericCodeBean.c0302}" />
						</p:selectOneMenu>
					</p:column>
				</p:row>
			</p:panelGrid>
		</div>
	</ui:define>
</ui:composition>